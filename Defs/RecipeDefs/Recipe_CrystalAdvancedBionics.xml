<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <RecipeDef Abstract="True" Name="SurgeryCrystalAdvanced" ParentName="SurgeryCrystalFlesh">
        <surgerySuccessChanceFactor>0.99</surgerySuccessChanceFactor>
        <deathOnFailedSurgeryChance>0.01</deathOnFailedSurgeryChance>
        <recipeUsers>
            <li>Alien_Crystalloid</li>
            <li>Alien_CrystalValkyrie</li>
            <li>Alien_Ascendant</li>
        </recipeUsers>
    </RecipeDef>

    <RecipeDef ParentName="SurgeryCrystalAdvanced">
        <defName>InstallCAdvancedPowerArm</defName>
        <label>install advanced power arm</label>
        <description>Installs advanced power arm.</description>
        <workerClass>Recipe_InstallArtificialBodyPart</workerClass>
        <jobString>Installing advanced power arm.</jobString>
        <workAmount>1000</workAmount>
        <ingredients>
            <li>
                <filter>
                    <categories>
                        <li>Medicine</li>
                    </categories>
                </filter>
                <count>1</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>AdvancedPowerArm</li>
                    </thingDefs>
                </filter>
                <count>1</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <categories>
                <li>Medicine</li>
            </categories>
            <thingDefs>
                <li>AdvancedPowerArm</li>
            </thingDefs>
        </fixedIngredientFilter>
        <appliedOnFixedBodyParts>
            <li>CrystalHumanLeftShoulder</li>
            <li>CrystalHumanRightShoulder</li>
        </appliedOnFixedBodyParts>
        <addsHediff>AdvancedPowerArm</addsHediff>
    </RecipeDef>

